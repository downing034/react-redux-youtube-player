{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "React boiler plate with testing setup",
  "main": "index.js",
  "repository": "git@github.com:downing034/react-boiler-plate.git",
  "scripts": {
    "lint": "eslint src test/jest --ext .js",
    "test": "jest",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "4.17.5",
    "moment": "^2.21.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "redux": "^3.0.4",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "7.1.4",
    "babel-preset-react": "^6.1.18",
    "chai": "4.1.2",
    "chai-jquery": "^2.0.0",
    "chance": "^1.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "jquery": "3.3.1",
    "jsdom": "11.6.2",
    "mocha": "5.0.4",
    "react-test-renderer": "^16.2.0",
    "rosie": "^1.6.0",
    "webpack": "4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "3.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "collectCoverage": false,
    "setupFiles": [
      "<rootDir>/test/jest/polyfills.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest/setupTests.js",
    "testMatch": [
      "<rootDir>/test/jest/**/*.test.js"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/test/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/test/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
